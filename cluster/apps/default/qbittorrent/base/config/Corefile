.:53 {
  bind 127.0.0.2
  rewrite stop type AAAA A
  errors
  health :8081 {
    lameduck 5s
  }
  log {
    class error
  }
  forward . tls://9.9.9.9 {
    tls_servername dns.quad9.net
    policy sequential
    health_check 5s
  }
  reload
}

